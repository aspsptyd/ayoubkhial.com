// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model PostInfo {
  id Int @id @default(autoincrement())
  slug String @unique
  views Int @default(1)
  likes Int @default(0)
}
